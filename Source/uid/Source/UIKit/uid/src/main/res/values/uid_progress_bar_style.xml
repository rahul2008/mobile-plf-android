<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="UIDProgressBarHorizontalDeterminate">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">@dimen/uid_progress_bar_min_width</item>
        <item name="android:indeterminate">false</item>
        <item name="uidIsLinearProgressBar">true</item>
        <item name="android:progressDrawable">@drawable/uid_secondary_progress_bar</item>
        <item name="android:mirrorForRtl">true</item>
    </style>

    <style name="UIDProgressBarHorizontalIndeterminate" parent="UIDProgressBarHorizontalDeterminate">
        <item name="android:indeterminate">true</item>
        <item name="android:indeterminateBehavior">repeat</item>
    </style>

    <style name="UIDProgressBarHorizontalDeterminateWithMargins" parent="UIDProgressBarHorizontalDeterminate">
        <item name="android:layout_marginLeft">@dimen/uid_progress_bar_margin_left</item>
        <item name="android:layout_marginRight">@dimen/uid_progress_bar_margin_right</item>
        <item name="android:indeterminate">false</item>
    </style>

    <style name="UIDProgressBarHorizontalIndeterminateWithMargins" parent="UIDProgressBarHorizontalDeterminateWithMargins">
        <item name="android:indeterminate">true</item>
        <item name="android:indeterminateBehavior">repeat</item>
    </style>

    <style name="UIDProgressBarCircularDeterminateSmall" parent="Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:layout_width">@dimen/uid_progress_bar_circular_small</item>
        <item name="android:layout_height">@dimen/uid_progress_bar_circular_small</item>
        <item name="android:minWidth">@dimen/uid_progress_bar_circular_small</item>
        <item name="android:minHeight">@dimen/uid_progress_bar_circular_small</item>
        <item name="android:indeterminate">false</item>
        <item name="uidIsLinearProgressBar">false</item>
        <item name="android:progressDrawable">@drawable/uid_circular_progress_bar_small</item>
        <item name="uidProgressBarCircularEndColor">?attr/uidTrackDefaultNormalOnBackgroundColor
        </item>
    </style>

    <style name="UIDProgressBarCircularIndeterminateSmall" parent="UIDProgressBarCircularDeterminateSmall">
        <item name="android:indeterminate">true</item>
        <item name="android:indeterminateBehavior">repeat</item>
        <item name="android:indeterminateDrawable">
            @drawable/uid_circular_progress_bar_small_indeterminate
        </item>
        <item name="android:indeterminateDuration">
            @integer/uid_progress_bar_circular_small_duration
        </item>
        <item name="uidProgressBarCircularEndColor">?attr/uidTrackDetailNormalOnBackgroundColor
        </item>
    </style>

    <style name="UIDProgressBarCircularDeterminateMiddle" parent="Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:layout_width">@dimen/uid_progress_bar_circular_middle</item>
        <item name="android:layout_height">@dimen/uid_progress_bar_circular_middle</item>
        <item name="android:minWidth">@dimen/uid_progress_bar_circular_middle</item>
        <item name="android:minHeight">@dimen/uid_progress_bar_circular_middle</item>
        <item name="android:indeterminate">false</item>
        <item name="uidIsLinearProgressBar">false</item>
        <item name="android:progressDrawable">@drawable/uid_circular_progress_bar_middle</item>
        <item name="uidProgressBarCircularEndColor">?attr/uidTrackDetailNormalOnBackgroundColor</item>
    </style>

    <style name="UIDProgressBarCircularIndeterminateMiddle" parent="UIDProgressBarCircularDeterminateMiddle">
        <item name="android:indeterminate">true</item>
        <item name="android:indeterminateBehavior">repeat</item>
        <item name="android:indeterminateDuration">
            @integer/uid_progress_bar_circular_middle_duration
        </item>
        <item name="android:indeterminateDrawable">
            @drawable/uid_circular_progress_bar_middle_indeterminate
        </item>
    </style>

    <style name="UIDProgressBarCircularDeterminateBig" parent="Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:layout_width">@dimen/uid_progress_bar_circular_big</item>
        <item name="android:layout_height">@dimen/uid_progress_bar_circular_big</item>
        <item name="android:minWidth">@dimen/uid_progress_bar_circular_big</item>
        <item name="android:minHeight">@dimen/uid_progress_bar_circular_big</item>
        <item name="android:indeterminate">false</item>
        <item name="uidIsLinearProgressBar">false</item>
        <item name="android:progressDrawable">@drawable/uid_circular_progress_bar_big</item>
        <item name="uidProgressBarCircularEndColor">?attr/uidTrackDetailNormalOnBackgroundColor</item>
    </style>

    <style name="UIDProgressBarCircularIndeterminateBig" parent="UIDProgressBarCircularDeterminateBig">
        <item name="android:indeterminate">true</item>
        <item name="android:indeterminateBehavior">repeat</item>
        <item name="android:indeterminateDuration">@integer/uid_progress_bar_circular_big_duration
        </item>
        <item name="android:indeterminateDrawable">
            @drawable/uid_circular_progress_bar_big_indeterminate
        </item>
    </style>
    
    <style name="UIDIndeterminateLinearProgress">
        <item name="uidIndeterminateLinearProgressBGColor">?attr/uidTrackDetailNormalOffBackgroundColor</item>
        <item name="uidIndeterminateLinearProgressStartColor">@android:color/transparent</item>
        <item name="uidIndeterminateLinearProgressCenterColor">?attr/uidTrackDetailNormalOnBackgroundColor</item>
        <item name="uidIndeterminateLinearProgressEndColor">@android:color/transparent</item>
        <item name="uidIndeterminateLinearProgressAnimDuration">@integer/uid_indeterminate_progressbar_anim_duration</item>
    </style>
</resources>